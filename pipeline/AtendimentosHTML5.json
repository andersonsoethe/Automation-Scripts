									"name": "CD_CEP"
								},
								"sink": {
									"name": "nr_cep",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "IE_SEXO"
								},
								"sink": {
									"name": "ie_sexo",
									"type": "String"
								}
							},
							{
								"source": {
									"name": "DT_NASCIMENTO"
								},
								"sink": {
									"name": "dt_nascimento",
									"type": "DateTime"
								}
							}
						]
					}
				},
